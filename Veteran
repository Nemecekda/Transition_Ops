const showToast = useCallback(msg => {
setToast(msg);
setTimeout(() => setToast(null), 3000);
}, []);
const toggleTask = useCallback((milestoneId, taskId) => {
setMilestones(prev => prev.map(m => m.id === milestoneId ? {
…m,
tasks: m.tasks.map(t => t.id === taskId ? {
…t,
done: !t.done
} : t)
} : m));
}, []);
const toggleDoc = useCallback(docId => {
setDocuments(prev => prev.map(d => d.id === docId ? {
…d,
obtained: !d.obtained
} : d));
}, []);
const getProgress = useCallback(m => {
const done = m.tasks.filter(t => t.done).length;
return {
done,
total: m.tasks.length,
pct: Math.round(done / m.tasks.length * 100)
};
}, []);
const getTotalProgress = useCallback(() => {
const all = milestones.flatMap(m => m.tasks);
const done = all.filter(t => t.done).length;
return {
done,
total: all.length,
pct: Math.round(done / all.length * 100)
};
}, [milestones]);
